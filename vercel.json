{
  "version": 2,
  "builds": [
    {
      "src": "wwwroot/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "/wwwroot/docs.html"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/wwwroot/docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/wwwroot/$1"
    }
  ]
}